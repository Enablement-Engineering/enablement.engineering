{
  "name": "Parchment Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1c1917",
    "editor.foreground": "#f5f5f4",
    "editorLineNumber.foreground": "#a8a29e",
    "editorLineNumber.activeForeground": "#f5f5f4"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#a8a29e",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#a3be8c"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.expression",
        "storage",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#d6a064"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "support.function",
        "meta.function-call"
      ],
      "settings": {
        "foreground": "#88c0d0"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "identifier"
      ],
      "settings": {
        "foreground": "#ebcb8b"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#ffd60a"
      }
    },
    {
      "name": "Constants",
      "scope": ["constant", "constant.other"],
      "settings": {
        "foreground": "#ffd60a"
      }
    },
    {
      "name": "Types and Classes",
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.type",
        "support.class"
      ],
      "settings": {
        "foreground": "#d6a064"
      }
    },
    {
      "name": "Properties",
      "scope": [
        "variable.other.property",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#e7c08a"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#a8a29e"
      }
    },
    {
      "name": "Tags (HTML/JSX)",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#d6a064"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#88c0d0"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#a3be8c"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ffd60a"
      }
    },
    {
      "name": "Imports",
      "scope": ["meta.import", "keyword.control.import"],
      "settings": {
        "foreground": "#d6a064"
      }
    },
    {
      "name": "Module Names",
      "scope": ["entity.name.module", "string.quoted.module"],
      "settings": {
        "foreground": "#a3be8c"
      }
    }
  ]
}
